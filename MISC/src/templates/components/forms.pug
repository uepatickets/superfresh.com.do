extends ../partials/layout-components

//- Override default config variables
append config
  - var vendorStyles = ['prismjs/themes/prism.css', 'prismjs/plugins/toolbar/prism-toolbar.css', 'prismjs/plugins/line-numbers/prism-line-numbers.css', 'nouislider/dist/nouislider.min.css']
  - var vendorScripts = ['prismjs/components/prism-core.min.js', 'prismjs/components/prism-markup.min.js', 'prismjs/components/prism-clike.min.js', 'prismjs/components/prism-javascript.min.js', 'prismjs/components/prism-pug.min.js', 'prismjs/plugins/toolbar/prism-toolbar.min.js', 'prismjs/plugins/copy-to-clipboard/prism-copy-to-clipboard.min.js', 'prismjs/plugins/line-numbers/prism-line-numbers.min.js', 'nouislider/dist/nouislider.min.js', 'cleave.js/dist/cleave.min.js']
  - var pageTitle = 'Forms'
  - var pageSubtitle = 'Form control styles, layout options, and custom components.'
  - var pageTitleLink = {'title': 'Bootstrap docs', 'link': 'https://getbootstrap.com/docs/5.1/forms/overview/'}
  - var anchorNav = [{'title': 'Supported input types', 'link': '#forms-types'}, {'title': 'Floating labels', 'link': '#forms-floating-labels'}, {'title': 'Password visibility toggle', 'link': '#forms-password-toggle'}, {'title': 'Checkboxes', 'link': '#forms-checkbox'}, {'title': 'Radios', 'link': '#forms-radios'}, {'title': 'Switches', 'link': '#forms-switch'}, {'title': 'Range slider', 'link': '#forms-range-slider'}, {'title': 'Sizing', 'link': '#forms-sizing'}, {'title': 'Readonly & disabled', 'link': '#forms-disabled'}, {'title': 'Inline form', 'link': '#forms-inline'}, {'title': 'Help text', 'link': '#forms-help-text'}, {'title': 'Validation: status text', 'link': '#forms-validation-text'}, {'title': 'Validation: status tooltips', 'link': '#forms-validation-tooltips'}, {'title': 'Format input text content', 'link': '#forms-input-format'}, {'title': 'Light version', 'link': '#forms-light'}]
  
//- Page Content
block content

  .alert.alert-info.mb-5(role='alert')
    .d-flex
      i.fi-alert-circle.lead.me-2.me-sm-3
      .text-break
        p.mb-2 <strong>Range slider</strong> depends on <strong>noUISlider plugin</strong>. Make sure to link to <strong>noUISlider css and js</strong> files in your document: <strong>vendor/nouislider/distribute/nouislider.min.css</strong> and <strong>vendor/nouislider/distribute/nouislider.min.js</strong>. Use this page as a reference.
        p.mb-1 To enable input text content formatting please make sure to link to <strong>Cleave.js plugin</strong> in your document: <strong>vendor/cleave.js/dist/cleave.min.js</strong>. Use this page as a reference.

  // Supported input types
  section#forms-types.pb-5.mb-md-2
    h2.h5.mb-3 Supported input types
    .card.border-0.shadow-sm
      .card-header
        ul(role='tablist').nav.nav-tabs.card-header-tabs
          li.nav-item
            a(href='#result1' data-bs-toggle='tab' role='tab' aria-controls='result1' aria-selected='true').nav-link.active Result
          li.nav-item
            a(href='#html1' data-bs-toggle='tab' role='tab' aria-controls='html1' aria-selected='false').nav-link HTML
          li.nav-item
            a(href='#pug1' data-bs-toggle='tab' role='tab' aria-controls='pug1' aria-selected='false').nav-link Pug
      .card-body
        .tab-content
          #result1.tab-pane.fade.show.active(role='tabpanel')
            .mb-3.row.align-items-center
              label(for='text-input').col-md-2.col-form-label Text
              .col-md-10
                input(type='text' id='text-input' value='Artisanal kale').form-control
            .mb-3.row.align-items-center
              label(for='search-input').col-md-2.col-form-label Search
              .col-md-10
                input(type='search' id='search-input' value='How do I shoot web').form-control
            .mb-3.row.align-items-center
              label(for='email-input').col-md-2.col-form-label Email
              .col-md-10
                input(type='email' id='email-input' value='email@example.com').form-control
            .mb-3.row.align-items-center
              label(for='url-input').col-md-2.col-form-label URL
              .col-md-10
                input(type='url' id='url-input' value='https://getbootstrap.com').form-control
            .mb-3.row.align-items-center
              label(for='tel-input').col-md-2.col-form-label Phone
              .col-md-10
                input(type='tel' id='tel-input' value='1-(770)-334-2518').form-control
            .mb-3.row.align-items-center
              label(for='pass-input').col-md-2.col-form-label Password
              .col-md-10
                input(type='password' id='pass-input' value='mypasswordexample').form-control
            .mb-3.row
              label(for='textarea-input').col-md-2.col-form-label Textarea
              .col-md-10
                textarea(id='textarea-input' rows='5').form-control Hello World!
            .mb-3.row.align-items-center
              label(for='select-input').col-md-2.col-form-label Select
              .col-md-10
                select(id='select-input').form-select
                  option Choose option...
                  option Option item 1
                  option Option item 2
                  option Option item 3
            .mb-3.row.align-items-center
              label(for='multiselect-input').col-md-2.col-form-label Multiselect
              .col-md-10
                select(id='multiselect-input' size='3' multiple).form-select
                  option Option item 1
                  option Option item 2
                  option Option item 3
                  option Option item 4
                  option Option item 5
                  option Option item 6
            .mb-3.row.align-items-center
              label(for='file-input').col-md-2.col-form-label File
              .col-md-10
                input(type='file' id='file-input').form-control
            .mb-3.row.align-items-center
              label(for='number-input').col-md-2.col-form-label Number
              .col-md-10
                input(type='number' id='number-input' value='37').form-control
            .mb-3.row.align-items-center
              label(for='datalist-input').col-md-2.col-form-label Datalist
              .col-md-10
                input(list='datalist-options' id='datalist-input' placeholder='Type to search...').form-control
                datalist#datalist-options
                  option(value='San Francisco')
                  option(value='New York')
                  option(value='Seattle')
                  option(value='Los Angeles')
                  option(value='Chicago')
            .mb-3.row.align-items-center
              label(for='range-input').col-md-2.col-form-label Range
              .col-md-10
                input(type='range' id='range-input').form-range
            .row.align-items-center
              label(for='color-input').col-md-2.col-form-label Color
              .col-md-10
                input(type='color' id='color-input' value='#5d3cf2').form-control.form-control-color
