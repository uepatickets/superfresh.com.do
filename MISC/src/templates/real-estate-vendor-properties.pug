extends partials/layout

//- Override default config variables
append config
  - var vendorStyles = ['tiny-slider/dist/tiny-slider.css']
  - var vendorScripts = ['tiny-slider/dist/min/tiny-slider.js']
  - var parentMenu = 'Vendor'
  - var pageTitle = 'Real Estate | Vendor Properties'

//- Navbar
block navbar
  include partials/navbar-real-estate

//- Page Content
block content

  // Message modal
  #message-modal.modal.fade(tabindex='-1')
    .modal-dialog.modal-dialog-centered(role='document')
      .modal-content
        .modal-header
          h3.fs-base.modal-title Message to Floyd Miles
          button(type='button' data-bs-dismiss='modal').btn-close.ms-0
        form(novalidate).modal-body.needs-validation
          .mb-4
            textarea(rows='6' placeholder='Type your message here' required).form-control
            .invalid-feedback Please type your message before sending.
          button(type='submit').btn.btn-primary.mb-2
            i.fi-send.me-2
            | Send message

  // Page container
  .container.mt-5.mb-md-4.py-5
    
    // Breadcrumb
    nav(aria-label='Breadcrumb').mb-4.pt-md-3
      ol.breadcrumb
        li.breadcrumb-item
          a(href='real-estate-home.html') Home
        li.breadcrumb-item
          a(href='#') Agents
        li(aria-current='page').breadcrumb-item.active
          | Floyd Miles
    
    .row

      // Sidebar (Agent's info)
      aside.col-lg-3.col-md-4.mb-5
        .pe-lg-3
          img(src='img/avatars/31.jpg' width='120' alt='Floyd Miles').d-block.rounded-circle.mx-auto.mx-md-0.mb-3
          h2.h4.text-center.text-md-start.mb-1 Floyd Miles
          p.text-center.text-md-start.mb-2.pb-1 Imperial Property Group Agent
          .d-flex.justify-content-center.justify-content-md-start.border-bottom.pb-4.mb-4
            +star-rating(5)
            a(href='real-estate-vendor-reviews.html').ms-2 45 reviews
          .border-bottom.pb-4.mb-4
            p.fs-sm.mb-0 Hello! I will select the best accommodation for you. Amet libero morbi venenatis ut est. Iaculis leo ultricies nunc id ante adipiscing. Vel metus odio at faucibus ac. Neque id placerat et id ut acultricies commodo.
            #seeMore.collapse
              p.fs-sm.mb-0 Sed ut perspiciatis unde omnis iste natus error sit voluptatem accusantium dolor laudantium, totam rem aperiam, eaque ipsa quae ab illo inventore veritatis.
            .text-center.text-md-start
              a(href='#seeMore' data-bs-toggle='collapse' data-bs-label-collapsed='Show more' data-bs-label-expanded='Show less' role='button' aria-expanded='false' aria-controls='seeMore').collapse-label.collapsed.fs-sm
          ul.d-table.list-unstyled.mx-auto.mx-md-0.mb-3.mb-md-4
            li.mb-2
              a(href='tel:3025550107').nav-link.fw-normal.p-0
                i.fi-phone.text-primary.mt-n1.me-2.align-middle
                | (302) 555-0107
            li
              a(href='mailto:floyd_miles@email.com').nav-link.fw-normal.p-0
                i.fi-mail.text-primary.mt-n1.me-2.align-middle
                | floyd_miles@email.com
          .text-center.text-md-start
            a(href='#').btn.btn-icon.btn-light-primary.btn-xs.rounded-circle.shadow-sm.me-2
              i.fi-facebook
            a(href='#').btn.btn-icon.btn-light-primary.btn-xs.rounded-circle.shadow-sm
              i.fi-linkedin
          .text-center.text-md-start.pt-md-2.mt-4
            a(href='#message-modal' data-bs-toggle='modal').btn.btn-primary
              i.fi-chat-left.fs-sm.me-2
              | Direct message


